      </div>
      <div
        id="total-entry-created"
      >
        <h4>
          Activity
        </h4>
        <span
          id="entries_created"
        />
        <div>
          <p
            className="joined date"
          >
            Joined 
            <span
              id="date-registered"
            />
          </p>
        </div>
      </div>
    </div>
  </div>
  <ChangePasswordModal />
</div>
`;

