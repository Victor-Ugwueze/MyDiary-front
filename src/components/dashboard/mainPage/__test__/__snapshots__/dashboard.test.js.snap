exports[`Renders modals should render the profile section of dashboard page 1`] = `
<div
  className="profile-section false tab-pane"
  id="profile"
>
  <div
    className="user-profile"
  >
    <div
      className="profile-details"
    >
      <div>
        <form
          encType="application/x-www-htmlform-urlencoded"
          id="update-profile"
        >
          <h4>
            Profile Details
          </h4>
          <div
            className="errors"
            id="profile-error"
          />
          <div
            className="item"
          >
            <label
              htmlFor="firstname"
            >
              First Name
            </label>
            <input
              className="htmlform-input"
              name="firstName"
              type="text"
            />
          </div>
          <div
            className="item"
          >
            <label
              htmlFor="lastname"
            >
              Last Name
            </label>
            <input
              className="htmlform-input"
              name="lastName"
              type="text"
            />
          </div>
          <div
            className="item"
          >
            <label
              htmlFor="email"
            >
              Email
            </label>
            <input
              className="htmlform-input"
              name="email"
              type="email"
            />
          </div>
          <div
            className="item"
          >
            <label
              htmlFor="location"
            >
              Location
            </label>
            <input
              className="htmlform-input"
              id="location"
              name="location"
              type="text"
            />
          </div>
          <button
            className="btn btn-primary change-password"
            data-target="update-password"
            type="button"
          >
            Change password
          </button>
