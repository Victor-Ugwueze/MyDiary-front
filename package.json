{
  "name": "mydiary-front",
  "version": "1.0.0",
  "description": "the front end for mydiary app",
  "main": "index.js",
  "author": "victor <victorugwueze@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint /src --fix",
    "start": "node app.js",
    "build": "webpack --config webpack.config.prod.js",
    "start:dev": "webpack-dev-server --config webpack.config.dev.js --progress --hot --open",
    "test": "jest --coverage",
    "postinstall": "yarn build",
    "coveralls": "jest --coverage && cat coverage/lcov.info | coveralls"
  },
  "jest": {
    "transform": {
      ".js$": "babel-7-jest",
      ".jsx$": "babel-7-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "axios-mock-adapter": "^1.15.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "bootstrap": "^4.1.3",
    "compression": "^1.7.3",
    "copy-webpack-plugin": "^4.6.0",
    "express": "^4.16.4",
    "history": "^4.7.2",
    "identity-obj-proxy": "^3.0.0",
    "jquery": "^3.3.1",
    "jsonwebtoken": "^8.3.0",
    "medium-editor": "^5.23.3",
    "nyc": "^13.1.0",
    "open": "^0.0.5",
    "path": "^0.12.7",
    "popper.js": "^1.14.4",
    "prop-types": "^15.6.2",
    "react": "^16.6.0",
    "react-contenteditable": "^3.1.1",
    "react-dom": "^16.6.0",
    "react-html-parser": "^2.0.2",
    "react-loader-spinner": "^2.2.0",
    "react-medium-editor": "^1.8.1",
    "react-modal": "^3.6.1",
    "react-redux": "^5.1.0",
    "react-router-dom": "^4.3.1",
    "react-share": "^2.3.1",
    "react-slick": "^0.23.2",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "serve-favicon": "^2.5.0",
    "slick-carousel": "^1.8.1",
    "sweetalert": "^2.1.2",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "axios-mock-adapter": "^1.15.0",
    "babel-7-jest": "^21.3.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "bootstrap": "^4.1.3",
    "clean-webpack-plugin": "^0.1.19",
    "coveralls": "^3.0.2",
    "css-loader": "^1.0.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "jsdom": "^13.0.0",
    "jsdom-global": "^3.0.2",
    "node-sass": "^7.0.0",
    "react-hot-loader": "^4.3.11",
    "react-test-renderer": "^16.6.0",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4"
  }
}
